{
  "name": "athe-sv",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "concurrently -k \"npm:dev:*\"",
    "dev:next": "next dev",
    "builds": "conc \"npm:build-*\" npm:build",
    "build": "next build",
    "start": "conc -k \"npm:start:*\"",
    "lint": "conc \"npm:lint:*\"",
    "start:next": "next start",
    "lint:next": "next lint",
    "dev:storybook": "npm run storybook",
    "build-storybook": "storybook build",
    "build-storydocs": "storybook build --docs",
    "bst": "concurrently \"npm:build:storybook\"",
    "ghci": "npm ci && conc \"npm:ghci:*\"",
    "chromatic": "npx chromatic --storybook-base-dir=noplus-web --build-script-name=build-storybook --exit-once-uploaded --exit-zero-on-changes --zip",
    "test": "conc \"npm:test:*\"",
    "test:jest": "jest",
    "storybook": "storybook dev -p 6006"
  },
  "dependencies": {
    "@gsap/react": "^2.1.1",
    "@octokit/graphql": "^8.1.1",
    "@radix-ui/react-accordion": "^1.1.2",
    "@radix-ui/react-alert-dialog": "^1.0.5",
    "@radix-ui/react-aspect-ratio": "^1.0.3",
    "@radix-ui/react-checkbox": "^1.0.4",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-popover": "^1.0.7",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-tooltip": "^1.0.7",
    "@sentry/nextjs": "^8.7.0",
    "axios": "^1.6.7",
    "clsx": "^2.1.0",
    "cmdk": "^1.0.0",
    "cva": "^1.0.0-beta.1",
    "date-fns": "^3.5.0",
    "gsap": "^3.12.5",
    "ldrs": "^1.0.1",
    "next": "14.2.3",
    "node-fetch": "^3.3.2",
    "octokit": "^3.2.1",
    "prettier": "^3.2.5",
    "react": "^18",
    "react-day-picker": "^8.10.0",
    "react-dom": "^18",
    "react-feather": "^2.0.10",
    "react-firebase-hooks": "^5.1.1",
    "react-google-recaptcha": "^3.1.0",
    "react-google-recaptcha-v3": "^1.10.1",
    "react-spinners": "^0.13.8",
    "sonner": "^1.4.41",
    "tailwind-merge": "^2.2.1",
    "tailwindcss-animate": "^1.0.7",
    "vaul": "^0.9.0"
  },
  "paths": {
    "@/*": [
      "./src/*"
    ],
    "@components/*": [
      "./src/components/*"
    ],
    "@ui/*": [
      "./src/components/ui/*"
    ],
    "@layout/*": [
      "./src/components/layout/*"
    ],
    "@story/*": [
      "./src/components/story/*"
    ],
    "@Sui/*": [
      "./src/components/story/ui/*"
    ],
    "@Slay/*": [
      "./src/components/story/layout/*"
    ],
    "@load/*": [
      "./src/components/ui/loaders/*"
    ],
    "@pre/*": [
      "./src/components/presets/*"
    ]
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.24.1",
    "@types/jest": "^29.5.12",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.4.17",
    "concurrently": "^8.2.2",
    "eslint": "^8",
    "eslint-config-next": "14.1.0",
    "eslint-plugin-storybook": "^0.8.0",
    "jest": "^29.7.0",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "ts-jest": "^29.1.2",
    "tsconfig-paths-webpack-plugin": "^4.1.0",
    "typescript": "^5"
  }
}
